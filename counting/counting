def count_charters(text):
    char_dict = {}
    for char in text:
        if char in char_dict.keys():
            char_dict[char] += 1 # char_dict[char] = char_dict.get[char, 0] + 1
        else:
            char_dict[char] = 1
        
    for char, count in char_dict.items():
        print char + " " + str(count

